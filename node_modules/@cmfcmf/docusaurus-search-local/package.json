{
  "name": "@cmfcmf/docusaurus-search-local",
  "version": "0.5.0",
  "description": "Local / Offline Search for Docusaurus",
  "keywords": [
    "docusaurus",
    "search",
    "plugin",
    "offline",
    "local"
  ],
  "license": "MIT",
  "repository": "cmfcmf/docusaurus-search-local",
  "main": "src/index.js",
  "files": [
    "/src",
    "!/src/generated.js",
    "!**/*.test.js",
    "/CHANGELOG.md",
    "/LICENSE",
    "/README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Christian Flach",
  "dependencies": {
    "autocomplete.js": "^0.38.0",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^1.1.1",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.4.0",
    "mark.js": "^8.11.1"
  },
  "peerDependencies": {
    "@docusaurus/core": "^2.0.0"
  },
  "devDependencies": {
    "cypress": "^4.11.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "lint": "prettier --check {src,cypress}/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}",
    "format": "prettier --write {src,cypress}/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,cypress}/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.9.0"
  }
}
